{
  "version": 2,
  "functions": {
    "server.js": {
      "runtime": "@vercel/node@latest",
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/public/favicon.ico"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
