{
  "version": 2,
  "builds": [
    {
      "src": "public/favicon.ico",
      "use": "@vercel/static"
    },
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/public/favicon.ico"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/server.js"
    }
  ],
  "functions": {
    "api/server.js": {
      "maxDuration": 900
    }
  }
}
