{
  "version": 2,
  "functions": {
    "api/server.js": {
      "runtime": "nodejs18.x",
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/public/favicon.ico"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/server.js"
    }
  ]
}
