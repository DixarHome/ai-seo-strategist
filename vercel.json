{
  "version": 2,
  "functions": {
    "api/server.js": {
      "runtime": "@vercel/node@16.14.x", // Specify the runtime version
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/public/favicon.ico"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/server.js"
    }
  ]
}
