{
  "version": 2,
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/public/favicon.ico"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/server.js",
      "methods": ["GET", "POST"],
      "headers": {
        "x-vercel-max-duration": "900"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/api/server.js"
    }
  ]
}
